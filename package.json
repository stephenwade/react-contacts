{
  "name": "react-contacts",
  "description": "An address book/contacts app written in React",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenwade/react-contacts.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "format:eslint": "eslint . --fix --ignore-path .gitignore",
    "format:stylelint": "stylelint \"**/*.css\" --fix --ignore-path .gitignore",
    "format": "npm run format:eslint; npm run format:stylelint"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.29.0",
    "stylelint": "^13.13.1",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.8"
  }
}
